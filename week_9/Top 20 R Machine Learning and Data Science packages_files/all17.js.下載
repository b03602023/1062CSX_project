function e17_write (nads) {
  var path=kpath + 'aps/';
  var ms=new Date().getMilliseconds(); var adshown=new Array();
  for (adpos=1; adpos<=nads; adpos++) {
    do { var adn=1; var re=Math.random()*8;
      while (Wta[adn]<re) {adn++};
    } while (adshown[adn]==1);
    if (We[adn]==1) { s=Ue[adn]; s=s.replace('ze/e-','ze/' + nads + adpos + ':e-'); 
      s+='<img src=' + path + Ne[adn] + '.gif?ms' + ms + ' width=1 height=1 border=0>';
    } else {
      s='<a href=' + Ue[adn] + ' onclick="_gaq.push([\'_trackPageview\',\'/ze/' + nads + adpos + ':' + Ne[adn] + '\']);" target=_blank>';
      s+='<img src=' + path + Ne[adn] + Ee[adn] + '?ms' + ms +  ' width=' + We[adn] + ' height=' + He[adn] + ' border=0 alt="' + Ae[adn] + '">';
      s+='<br><b>' + Te[adn] + '</b></a><br>';}
    if (nads>1) {s+='<br><hr class="grey-line"><br>'};
    document.writeln(s); adshown[adn]=1;
  }
}
